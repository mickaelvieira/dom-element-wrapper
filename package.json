{
  "name": "dom-element-wrapper",
  "description": "Make the DOM great again!",
  "version": "0.0.3",
  "main": "dist/dom-element-wrapper.js",
  "js:next": "dist/dom-element-wrapper-es.js",
  "module": "dist/dom-element-wrapper-es.js",
  "repository": "git@github.com:mickaelvieira/dom-element-wrapper.git",
  "homepage": "https://github.com/mickaelvieira/dom-element-wrapper",
  "author": "Mickael Vieira <contact@mickael-vieira.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mickaelvieira/dom-element-wrapper/issues"
  },
  "keywords": [
    "dom",
    "api",
    "wrapper",
    "es6"
  ],
  "scripts": {
    "clean": "rm -rf ./dist",
    "fmt": "prettier --write ./src/*.js && prettier --write ./spec/*.js && prettier --write rollup.config.js",
    "lint": "eslint --quiet -c .eslintrc src",
    "test": "NODE_ENV=test jest --config=jest.json",
    "test:watch": "yarn run test -- --watch",
    "test:coverage": "yarn run test -- --coverage",
    "test:coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "watch": "rollup -c rollup.config.js --watch",
    "build": "rollup -c rollup.config.js",
    "prepublish": "npm run clean && npm test && npm run build"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-jest": "^20.0.3",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "coveralls": "^2.13.1",
    "eslint": "^4.1.1",
    "eslint-plugin-prettier": "^2.1.2",
    "jest": "^20.0.4",
    "prettier": "^1.5.2",
    "rollup": "^0.43.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^4.0.0",
    "uglify-es": "^3.0.23"
  },
  "dependencies": {}
}
